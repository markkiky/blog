                    <!-- unfreeze account -->
        <div class="modal fade" id="deactivate-room" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
            <div class="modal-dialog modal-dialog-centered" role="document">
                <div class="modal-content">
                    <div class="modal-header">
                        <h5 class="modal-title" id="exampleModalLongTitle">Deactivate Room 104</h5>
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                    </div>
                    <div class="modal-body">
                        <div class="">
                            <h5>Are you sure you want to deactivate this room?</h5>
                        </div>
                        <div class="row">
                            <div class="col-12">
                                <div class="form-group">
                                    <label>Reason<strong class="text-danger">*</strong></label>
                                    <textarea class="form-control validate"  minlength="2" placeholder="Enter reason for room deactivation" required></textarea>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                        <button type="button" class="btn btn-success">Yes, Deactivate Room</button>
                    </div>
                </div>
            </div>
        </div>
            
            
			 <section class="content">
                <header class="content__title px-0 border-0">                    
				 	<div class="row">
						<div class="col-lg-6 col-sm-12 col-md-6">
							<span class="rev-title-container">
							 <h1 class="text-capitalize stream_name m-0">Available Rooms</h1>
							</span>
							<div class="">
								<ol class="breadcrumb border-0">
                                    <li class="breadcrumb-item">Home</li>
                                    <li class="breadcrumb-item">Rooms</li>
									<li class="breadcrumb-item active">Available Rooms</li>
								
								</ol>
								
							</div>
                        </div>
                        <div class="col-lg-6 col-sm-12 col-md-6">
                            <a></a>
                        </div>

					</div>
                </header>

                <div class="row">
