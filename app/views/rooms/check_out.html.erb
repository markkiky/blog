    <body data-ma-theme="green">
        <main class="main">
            <%= render "layouts/page_loader" %>

            <%= render "layouts/page_header" %>
            
			<%= render "layouts/page_sidebar" %>

            <!-- Modals -->
            <div class="modal fade" id="edit_booking_details" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
                <div class="modal-dialog modal-dialog-centered" role="document">
                    <div class="modal-content">
                        <div class="modal-header">
                        <h5 class="modal-title" id="exampleModalLongTitle">Edit Booking Details</h5>
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                            <span aria-hidden="true">&times;</span>
                        </button>
                        </div>
                        <div class="modal-body">
                            <div>
                                <div class="form-group">
                                    <label for="booking_code">Booking Code <strong class="text-danger">*</strong></label>
                                    <input type="text" value="RS-202014502" id="booking_code" class="form-control posta-form validate" pattern="^[0-9']{1,100000000000000000}$" minlength="1" required placeholder="Enter postal code">
                                </div>

                                <div class="form-group">
                                    <label for="reservation_date">Date of Reservation <strong class="text-danger">*</strong></label>
                                    <input type="text" class="form-control date-picker  dob1" value="4th Aug 2020: 12:30PM" id="reservation_date" placeholder="Enter date of reservation">
                                    <input type="date" class="form-control hidden-md-up dob2 d-none" placeholder="Pick a date">
                                </div>

                                <div class="form-group">
                                    <label>Reservation Type <strong class="text-danger">*</strong></label>
                                    <select class="form-control selectpicker show-tick" data-live-search="true">
                                        <option value="Delux">Delux</option>
                                        <option value="Business">Business</option>
                                        <option value="Special">Special</option>
                                    </select>
                                </div>

                                <div class="form-group">
                                    <label for="reservation_details">Reservation Details <strong class="text-danger">*</strong></label>
                                    <input type="text" class="form-control" value="2 Beds and Kitchen" id="reservation_details" placeholder="Enter reservation details">
                                </div>

                                <div class="form-group">
                                    <label for="reservation_duration">Reservation Duration <strong class="text-danger">*</strong></label>
                                    <input type="text" class="form-control" value="4 nights" id="reservation_duration" placeholder="Enter reservation duration">
                                </div>

                                <div class="form-group">
                                    <small class="text-muted-blue bold">Reservation Request</small>
                                    <ul class="modal-check-container">
                                            <label class="modal-container">Crib
                                                <input type="checkbox">
                                                <span class="modal-checkmark"></span>
                                                </label>                                        

                                            <label class="modal-container">Cooking Utensils
                                                <input type="checkbox">
                                                <span class="modal-checkmark"></span>
                                                </label>

                                            <label class="modal-container">Extra Towels
                                                <input type="checkbox">
                                                <span class="modal-checkmark"></span>
                                            </label>

                                            <label class="modal-container">Diaper Disposal
                                                <input type="checkbox">
                                                <span class="modal-checkmark"></span>
                                            </label>   
                                        </ul>
                                </div>

                                <div class="form-group">
                                    <small class="text-muted-blue bold">Suggested Amount</small>
                                    <h4><strong>KES 30,250</strong></h4>
                                </div>                               
                                
                            </div>                       
                        </div>
                        <div class="modal-footer">
                        
                        <button type="button" class="btn btn-danger btn--icon-text px-5">Cancel</button>
                        <button type="button" class="btn btn-success btn--icon-text px-5">Save</button>

                        </div>
                    </div>
                </div>
            </div>
