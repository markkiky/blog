                                <div class="contact-header">
                                    <h4>Room Search</h4>
                                    <hr>
                                </div>

                                <div class="form-group">
                                    <label>Room No.<strong class="text-danger">*</strong></label>
                                    <p><small>Enter room number of the occupant checking out</small></p>
                                    <input type="room" class="form-control validate" id="id1" required placeholder="Room Number">

                                </div>

                                    <button onclick="checkout_details()" class="btn float-right room-search-btn text-white btn-border mr-3"><i class="mr-2 zmdi zmdi-search"></i>Search </button>

                            </div>
                        </div>
                    </div>
                </div>
                
                <div class="row checkout_content d-none">

                    <div class="col-xl-4 col-lg-4 col-md-4 col-sm-12">
                        <div class="row bill-amount-row">
